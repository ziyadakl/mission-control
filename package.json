{
  "name": "mission-control",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "tsx src/lib/db/migrate.ts",
    "db:seed": "tsx src/lib/db/seed.ts",
    "db:backup": "sqlite3 mission-control.db 'PRAGMA wal_checkpoint(TRUNCATE);' && cp mission-control.db mission-control.db.backup && echo 'Backed up to mission-control.db.backup'",
    "db:restore": "cp mission-control.db.backup mission-control.db && echo 'Restored from backup'",
    "db:reset": "rm -f mission-control.db mission-control.db-* && npm run db:seed"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^17.0.0",
    "better-sqlite3": "^11.7.0",
    "clsx": "^2.1.1",
    "css-tree": "^3.1.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.468.0",
    "next": "14.2.21",
    "playwright": "^1.58.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-js": "^1.2.1",
    "uuid": "^11.0.4",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/css-tree": "^2.3.11",
    "@types/node": "^20.17.12",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.21",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
